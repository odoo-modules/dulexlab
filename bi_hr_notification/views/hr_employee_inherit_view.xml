<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="bi_hr_employee_form_inherit_view" model="ir.ui.view">
            <field name="name">bi.hr.employee.form.inherit</field>
            <field name="model">hr.employee</field>
            <field name="inherit_id" ref="hr.view_employee_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='identification_id']" position="after">
                    <field name="id_expiry_date"/>
                </xpath>
                <xpath expr="//field[@name='notes']" position="before">
                    <group>
                        <group name="notify" string="Notification">
                            <field name="linked_absence"/>
                            <field name="unlinked_absence"/>
                        </group>
                    </group>
                </xpath>
            </field>
        </record>
    </data>
</odoo>