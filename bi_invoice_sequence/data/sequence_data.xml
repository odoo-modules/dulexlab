<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="untaxed_inv_sequence" model="ir.sequence">
            <field name="name">Un-Taxed Invoice Sequence</field>
            <field name="code">untaxed.invoice.seq</field>
            <field name="padding">5</field>
            <field name="prefix">UTI/</field>
        </record>
        <record id="taxed_inv_sequence" model="ir.sequence">
            <field name="name">Taxed Invoice Sequence</field>
            <field name="code">taxed.invoice.seq</field>
            <field name="padding">5</field>
            <field name="prefix">TIS/</field>
        </record>
    </data>
</odoo>
